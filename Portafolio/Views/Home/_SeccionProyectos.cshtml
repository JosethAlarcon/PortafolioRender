@model IEnumerable<ProyectoDTO>

@if (Model is not null)
{
    <div class="text-center" style="padding-top: 3rem;">
        <h1>Mis proyectos recientes</h1>
        <p>Aqui puedes ver algunos de mis proyectos mas recientes</p>
    </div>

    <partial name="_ListadoProyectos" model="Model" />

    <div class="text-center mb-3">
        <a class="btn btn-primary btn-personalizado" asp-controller="Home" asp-action="Proyectos">Mas Proyectos</a>
    </div>
}
